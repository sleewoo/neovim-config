{
  "import from node": {
    "prefix": ["i/n"],
    "body": ["import { $2 } from \"node:$1\";"]
  },
  "import from ./": {
    "prefix": ["i/."],
    "body": ["import { $2 } from \"./$1\";"]
  },
  "import default from ./": {
    "prefix": ["id/."],
    "body": ["import $1 from \"./$2\";"]
  },
  "import from ../": {
    "prefix": ["i/.."],
    "body": ["import { $2 } from \"../$1\";"]
  },
  "import default from ../": {
    "prefix": ["id/.."],
    "body": ["import $1 from \"../$2\";"]
  },
  "import from ~/": {
    "prefix": ["i/~"],
    "body": ["import { $2 } from \"~/$1\";"]
  },
  "import default from ~/": {
    "prefix": ["id/~"],
    "body": ["import $1 from \"~/$2\";"]
  },
  "import from @/": {
    "prefix": ["i/@"],
    "body": ["import { $2 } from \"@/$1\";"]
  },
  "import default from @/": {
    "prefix": ["id/@"],
    "body": ["import $1 from \"@/$2\";"]
  }
}
